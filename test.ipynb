    "n = int(input('Quantos termos você quer mostrar?'))\n",
    "t1 = 0\n",
    "t2 = 1\n",
    "print('-'*5*n)\n",
    "print(f'{t1} → {t2} →', end=\" \")\n",
    "cont = 3\n",
    "while cont <= n:\n",
    "    t3 = t1 + t2\n",
    "    print(f'{t3} → ', end=\"\")\n",
    "    t1 = t2\n",
    "    t2 = t3\n",
    "    cont += 1\n",
    "print('FIM')"
